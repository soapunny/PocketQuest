    "dev:server": "pnpm --filter ./apps/server dev",
    "dev": "pnpm -r --parallel dev",
    "db:generate": "pnpm --filter @pocketquest/prisma generate",
    "db:migrate": "pnpm --filter @pocketquest/prisma migrate",
    "db:studio": "pnpm --filter @pocketquest/prisma studio"
